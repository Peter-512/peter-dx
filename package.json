{
  "name": "peter-dx",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "supabase start && vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "db:migration:new": "supabase migration new",
    "db:migration:auto": "supabase db diff --use-migra -f",
    "db:types": "supabase gen types typescript --local --schema public > src/lib/types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:stop": "supabase stop"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.2.0",
    "@sveltejs/enhanced-img": "^0.2.0",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/typography": "^0.5.12",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "mdsvex": "^0.11.0",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "shiki": "^1.3.0",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.9",
    "svelte-headless-table": "^0.18.2",
    "svelte-radix": "^1.1.0",
    "sveltekit-view-transition": "^0.5.3",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.4.4",
    "vite": "^5.2.8"
  },
  "type": "module",
  "dependencies": {
    "@supabase/supabase-js": "^2.42.0",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "bits-ui": "^0.21.2",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "lucide-svelte": "^0.366.0",
    "radix-icons-svelte": "^1.2.1",
    "svelte-markdown": "^0.4.1",
    "svelte-vertical-timeline": "^1.0.0",
    "tailwind-merge": "^2.2.2",
    "tailwind-variants": "^0.2.1"
  }
}